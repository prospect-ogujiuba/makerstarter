{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 3,
  "settings": {
    "appearanceTools": true,
    "border": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true
    },
    "color": {
      "background": true,
      "custom": true,
      "customDuotone": true,
      "customGradient": true,
      "defaultDuotone": true,
      "defaultGradients": true,
      "defaultPalette": true,
      "duotone": [],
      "gradients": [],
      "link": true,
      "palette": [],
      "button": true,
      "caption": true,
      "heading": true,
      "text": true
    },
    "custom": {},
    "dimensions": { "aspectRatio": true, "minHeight": true },
    "layout": {
      "allowCustomContentAndWideSize": true,
      "allowEditing": true,
      "contentSize": "800px",
      "wideSize": "1100"
    },
    "position": {},
    "shadow": {},
    "spacing": {},
    "typography": {},
    "useRootPaddingAwareAlignments": false,
    "blocks": {
      "core/image": {
        "lightbox": {
          "enabled": true,
          "allowEditing": true
        }
      }
    }
  },
  "templateParts": [
  ]
}
